int led1 = 2; int 
led2 = 3; int 
led3 = 4; 
void setup() { // Set LED 
pins as output 
pinMode(led1, OUTPUT); 
pinMode(led2, OUTPUT); 
pinMode(led3, OUTPUT); 
} 
void loop() { // Turn on 
LED1 digitalWrite(led1, 
HIGH); delay(500); 
digitalWrite(led1, LOW); 
 // Turn on LED2 
digitalWrite(led2, HIGH); 
delay(500); digitalWrite(led2, 
LOW); 
 // Turn on LED3 
digitalWrite(led3, HIGH);
delay(500); digitalWrite(led3,
LOW);
} 
